{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown", "plugins/verboseOutput"],
    "markdown": {
        "parser": "gfm"
    },
    "explain":true,
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "SIMON":"SIMON"
    },
    "css":{
        "foo":"/foo.css"
    },
    "jsVersion": 180,
    "layout":"jsplumb-apidoc.tmpl"
}